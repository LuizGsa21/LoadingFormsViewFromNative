<?xml version="1.0" encoding="UTF-8"?>
<ContentView xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="LoadingFormsViewFromNative.CommonFormsView"	>
	<ContentView.Content>
		<StackLayout Padding="10" HorizontalOptions="FillAndExpand" VerticalOptions="FillAndExpand">
			<Label TextColor="Black" Text="This is a Forms view"  HorizontalOptions="FillAndExpand" HeightRequest="40" ></Label>
